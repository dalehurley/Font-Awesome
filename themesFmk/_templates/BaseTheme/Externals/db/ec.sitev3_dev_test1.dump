INSERT INTO `sid_groupe_sites_utiles_translation` (`id`, `title`, `description`, `lang`) VALUES (1,'Les sites d\'aide aux entreprises','<p>\r\n	Quelques sites simplifiant la vie de l&#39;entreprise au quotidien.</p>\r\n<p>\r\n	<em>T&eacute;nor est un mod&egrave;le de site internet con&ccedil;u par EXPERT INFOS sp&eacute;cifiquement pour les cabinets d&#39;expertise comptable. Il inclut des rubriques sur-mesure comme cette rubrique pr&eacute;sentant des sites utiles que vous pilotez directement de votre ordinateur. Contactez nos &eacute;quipes pour en savoir plus&nbsp; : <a href=\"mailto:contact@expertinfos.com?subject=Demande%20d%27information%20suite%20visite%20du%20site%20internet%20TENOR\">contact@expertinfos.com</a> / 05 49 60 26 47</em></p>\r\n','fr'),(5,'Les sites gouvernementaux','<p>\r\n	Ici nous mettrons &agrave; disposition les liens vers les diff&eacute;rents sites du gouvernement.</p>\r\n<p>\r\n	<em>T&eacute;nor est un mod&egrave;le de site internet con&ccedil;u par EXPERT INFOS sp&eacute;cifiquement pour les cabinets d&#39;expertise comptable. Il inclut des rubriques sur-mesure comme cette rubrique pr&eacute;sentant des sites utiles que vous pilotez directement de votre ordinateur. Contactez nos &eacute;quipes pour en savoir plus&nbsp; : <a href=\"mailto:contact@expertinfos.com?subject=Demande%20d%27information%20suite%20visite%20du%20site%20internet%20TENOR\">contact@expertinfos.com</a> / 05 49 60 26 47</em></p>\r\n','fr'),(6,'Les sites dédiés aux BNC','<p>\r\n	Professionnels lib&eacute;raux, vous trouverez dans cette rubrique les liens pour vos t&eacute;l&eacute;clarations et les sites concernant votre protection sociale.</p>\r\n<p>\r\n	Si vous avez d&#39;autres suggestions de sites int&eacute;ressants, n&#39;h&eacute;sitez pas &agrave; contacter St&eacute;phanie Lesage, notre assistante de direction, pour lui en faire part : <a href=\"mailto:contact@expertinfos.com?subject=Test%20effectu%C3%A9%20sur%20le%20site%20de%20d%C3%A9monstration%20T%C3%A9nor%