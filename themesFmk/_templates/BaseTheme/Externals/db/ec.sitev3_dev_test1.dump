INSERT INTO `sid_cabinet_equipe_translation` (`id`, `title`, `statut`, `tel`, `fax`, `gsm`, `email`, `text`, `lang`, `version`) VALUES (1,'Monsieur Legrand','Expert-Comptable Associé','05 49 60 20 61','','','legrand@lesiteducabinetconcerto.com','<p>\r\n	Dipl&ocirc;m&eacute; de l&#39;Institut du droit des affaires de l&#39;universit&eacute; Paris-II, Monsieur Legrand est co-fondateur du cabinet, Expert-comptable et Commissaire aux comptes. Il est en charge des activit&eacute;s Cr&eacute;ation d&rsquo;entreprise, Evaluation, Consolidation, Syst&egrave;mes d&rsquo;information, Outsourcing et Formation.</p>\r\n','fr',1),(2,'Monsieur Lefort','Expert-Comptable Associé','01 23 45 67 89','','','lefort@lesiteducabinetconcerto.com','<p>\r\n	Monsieur Lefort a fond&eacute; le cabinet avec M.Legrand, apr&egrave;s une formation en science de gestion, l&rsquo;obtention du DSCG et DEC et plusieurs exp&eacute;riences dans les plus grands cabinets et r&eacute;seaux. Vrai g&eacute;n&eacute;raliste de l&rsquo;expertise comptable, il manage nos diff&eacute;rents collaborateurs sp&eacute;cialis&eacute;s sur Paris.</p>\r\n','fr',1),(3,'Madame Lexperte','Expert-Comptable','05 49 60 20 63','','','lexpert@lesiteducabinetconcerto.com','<p>\r\n	Associ&eacute;e, Expert-comptable et Commissaire aux comptes, Madame Lexperte a rejoint le cabinet en 2007 pour prendre en charge les activit&eacute;s Expertise-comptable, Audit, Conseil en mati&egrave;re sociale et fiscale, Assistance juridique et Transmissions d&rsquo;entreprises.</p>\r\n','fr',1);
