INSERT INTO `sid_actu_article_translation` (`id`, `title`, `resume`, `text`, `lang`, `version`) VALUES (6,'Actualité Cabinet','Ténor, le site conçu par EXPERT INFOS pour les gestionnaires de patrimoine, comprend une rubrique auto-administrable où vous pouvez, directement de votre ordinateur, informer vos clients et prospects de votre actualité.','<p>\r\n	&nbsp;</p>\r\n<p>\r\n	<strong>NVITATION SALON DU PATRIMOINE PRIVE DE LYON</strong></p>\r\n<p>\r\n	<strong>Les 3 et 4 f&eacute;vrier 2012, nous serons pr&eacute;sents au Salon du Patrimoine Priv&eacute; R&eacute;gional</strong>,&nbsp;&nbsp;qui se d&eacute;roulera au Centre des Congr&egrave;s (cit&eacute; internationale) de Lyon.</p>\r\n<p>\r\n	Le Salon du Patrimoine Priv&eacute; R&eacute;gional destin&eacute; aux particuliers permet de faire connaitre au grand public les nouveaux produits de la gestion priv&eacute;e et rencontrer imm&eacute;diatement des professionnels et consultants gratuitement.</p>\r\n<p>\r\n	Venez nous rejoindre sur notre stand (T25) et participez &agrave; notre atelier sur<strong>&nbsp;l&#39;Optimisation fiscale de la r&eacute;cup&eacute;ration de tr&eacute;sorerie&nbsp; d&#39;une entreprise dans le patrimoine priv&eacute;</strong>&nbsp;(le 4 f&eacute;vrier - 10h45 - Salle Bleue).</p>\r\n<p>\r\n	Nous vous accueillerons avec plaisir et mettons &agrave; votre disposition d&egrave;s aujourd&#39;hui des invitations gratuites pour acc&eacute;der au salon et aux conf&eacute;rences.</p>\r\n<p>\r\n	&nbsp;</p>\r\n<p>\r\n	<strong>Contactez-nous pour en savoir plus :<br />\r\n	T&eacute;l : &nbsp;05 49 60 26 47&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<br />\r\n	Email :&nbsp;<a href=\"mailto:secretariat@le-site-du-cabinet-tenor.com\" target=\"_blank\">secretariat@le-site-du-<wbr>cabinet-tenor.com</wbr></a></strong></p>\r\n','fr',5),(7,'Actualité Cabinet 2','Le cabinet accueille un nouveau collaborateur !','<div>\r\n	Pierre Dupont vient en effet de nous rejoindre pour renforcer nos &eacute;quipes.&nbsp; Pierre est dipl&ocirc;m&eacute; du DESCF. Il a acquis, &agrave; 36 ans, une solide exp&eacute;rience en conseil&nbsp; aux entreprises pour avoir d&eacute;j&agrave; exerc&eacute; dans 3 cabinets.&nbsp; Il sera en appui de Caroline Martin, en tant que Chef de mission.&nbsp; Vous pouvez le joindre d&egrave;s &agrave; pr&eacute;sent sur sa ligne directe&nbsp;(05 49 60 26 47) ou par email (<a href=\"mailto:pierre.dupont@siteducabinet.com\">pierre.dupont@siteducabinet.com</a>)<br />\r\n	&nbsp;</div>\r\n<div>\r\n	<em>T&eacute;nor est un mod&egrave;le de site internet con&ccedil;u par EXPERT INFOS sp&eacute;cifiquement pour les cabinets d&#39;expertise comptable. Il inclut des rubriques sur-mesure comme cette page pr&eacute;sentant l&#39;actualit&eacute; du Cabinet que vous pilotez directement de votre ordinateur. Contactez nos &eacute;quipes pour en savoir plus&nbsp; : <a href=\"mailto:contact@expertinfos.com?subject=Demande%20d%27information%20suite%20visite%20du%20site%20internet%20TENOR\">contact@expertinfos.com</a> / 05 49 60 26 47</em></div>\r\n','fr',1);
