INSERT INTO `sid_cabinet_mission_translation` (`id`, `title`, `text`, `lang`, `version`) VALUES (1,'Toutes nos missions','<div id=\"content-area\">\r\n	<p>\r\n		L&#39;expert-comptable, <b>sp&eacute;cialiste pluridisciplinaire</b>, dispose de comp&eacute;tences &eacute;tendues pour vous suivre et vous conseiller durant toute la vie de votre entreprise.</p>\r\n	<p>\r\n		Notre cabinet met &agrave; votre disposition un <b>savoir-faire</b> reconnu en mati&egrave;re <b>financi&egrave;re, fiscale, sociale, juridique, informatique et de gestion</b>.<br />\r\n		En relation constante avec les organes d&eacute;cisionnels de votre entreprise, il vous assure une prestation fiable et rapide ainsi qu&#39;un conseil permanent dans les domaines cruciaux de la vie de votre entreprise. Nous nous impliquons directement dans votre strat&eacute;gie de r&eacute;ussite et nous nous engageons &agrave; vos c&ocirc;t&eacute;s afin de contribuer &agrave; faire de votre structure une entreprise dynamique et adapt&eacute;e aux besoins de votre client&egrave;le.</p>\r\n</div>\r\n','fr',7),(2,'Nos missions comptables','<p>\r\n	&nbsp;<span class=\"Apple-style-span\" style=\"font-size: 10px; font-weight: bold;\">Participation &agrave; l&#39;&eacute;tablissement des comptes :</span></p>\r\n\r\n	<ul>\r\n		<li>\r\n			Mise en place du Plan comptable adapt&eacute; &agrave; l&#39;entreprise&nbsp;</li>\r\n		<li>\r\n			Surveillance, tenue de comptabilit&eacute;s&nbsp;;</li>\r\n		<li>\r\n			Contr&ocirc;le des services comptables&nbsp;;</li>\r\n		<li>\r\n			Mise en place et suivi de la comptabilit&eacute; analytique d&#39;exploitation&nbsp;;</li>\r\n		<li>\r\n			Assistance comptable de toute nature&nbsp;;</li>\r\n		<li>\r\n			Etablissement des situations p&eacute;riodiques, des bilans et comptes de r&eacute;sultat&nbsp;;</li>\r\n		<li>\r\n			D&eacute;livrance du visa aux adh&eacute;rents des centres de gestion agr&eacute;&eacute;s&nbsp;;</li>\r\n		<li>\r\n			Etablissement de situations pr&eacute;visionnelles&nbsp;;</li>\r\n		<li>\r\n			Etablissement des comptes de groupes, consolidation des bilans et des comptes&nbsp;;</li>\r\n		<li>\r\n			&Eacute;tablissement du tableau de financement&nbsp;;</li>\r\n		<li>\r\n			Assistance du comit&eacute; d&#39;entreprise.</li>\r\n	</ul>\r\n	<h5>\r\n		R&eacute;vision, certification :</h5>\r\n	<ul>\r\n		<li>\r\n			R&eacute;vision contractuelle des comptes&nbsp;;</li>\r\n		<li>\r\n			R&eacute;vision des proc&eacute;dures de contr&ocirc;le interne&nbsp;;</li>\r\n		<li>\r\n			R&eacute;vision l&eacute;gale des comptes (commissariat aux comptes)&nbsp;;</li>\r\n		<li>\r\n			R&eacute;vision avant prise de participation&nbsp;;</li>\r\n		<li>\r\n			R&eacute;vision pr&eacute;alable &agrave; l&#39;introduction en Bourse des titres d&#39;une soci&eacute;t&eacute;&nbsp;;</li>\r\n		<li>\r\n			Commissariat aux apports.</li>\r\n	</ul>\r\n	<h5>\r\n		Expertise :</h5>\r\n	<ul>\r\n		<li>\r\n			Expertise amiable&nbsp;;</li>\r\n		<li>\r\n			Expertise judiciaire&nbsp;;</li>\r\n		<li>\r\n			Arbitrage&nbsp;;</li>\r\n		<li>\r\n			&Eacute;valuation d&#39;entreprises et de titres de soci&eacute;t&eacute;s (notamment en vue des ventes, cessions, apports, successions, partages, donations ou &agrave; la demande d&#39;organismes divers)&nbsp;;</li>\r\n		<li>\r\n			Expertise de gestion.</li>\r\n	</ul>\r\n','fr',10),(3,'Nos missions juridiques et fiscales','<h5>\r\n	Droit des affaires :</h5>\r\n<div>\r\n	<div>\r\n		<ul>\r\n			<li>\r\n				Contrats et baux&nbsp;;</li>\r\n			<li>\r\n				L&eacute;gislation des ententes&nbsp;;</li>\r\n			<li>\r\n				L&eacute;gislation des prix&nbsp;;</li>\r\n			<li>\r\n				Droit p&eacute;nal des affaires.</li>\r\n		</ul>\r\n		<h5>\r\n			Droit des soci&eacute;t&eacute;s :</h5>\r\n		<ul>\r\n			<li>\r\n				Choix de la forme sociale&nbsp;;</li>\r\n			<li>\r\n				R&eacute;alisation de toutes op&eacute;rations (constitution, augmentation de capital, cession de parts, fusion, transformation, scission, dissolution, liquidation)&nbsp;;</li>\r\n			<li>\r\n				Secr&eacute;tariat de soci&eacute;t&eacute;s : comptes-rendus de conseils d&#39;administration et d&#39;assembl&eacute;es g&eacute;n&eacute;rales et formalit&eacute;s aff&eacute;rentes.</li>\r\n		</ul>\r\n		<h5>\r\n			Droit de l&#39;entreprise individuelle :</h5>\r\n		<ul>\r\n			<li>\r\n				Droit patrimonial&nbsp;;</li>\r\n			<li>\r\n				Pr&eacute;voyance sociale du chef d&#39;entreprise&nbsp;;</li>\r\n			<li>\r\n				Transmission.</li>\r\n		</ul>\r\n		<h5>\r\n			Droit fiscal :</h5>\r\n		<ul>\r\n			<li>\r\n				&Eacute;tudes et consultations sur les probl&egrave;mes d&#39;ordre fiscal&nbsp;;</li>\r\n			<li>\r\n				Participation &agrave; l&#39;&eacute;tablissement des d&eacute;clarations fiscales :\r\n				<ul>\r\n					<li>\r\n						Imp&ocirc;ts directs&nbsp;;</li>\r\n					<li>\r\n						TVA et taxes assimil&eacute;es&nbsp;;</li>\r\n					<li>\r\n						Fiscalit&eacute; immobili&egrave;re&nbsp;;</li>\r\n					<li>\r\n						Droits d&#39;enregistrement et droits de mutation&nbsp;;</li>\r\n					<li>\r\n						Douanes&nbsp;;</li>\r\n					<li>\r\n						Autres imp&ocirc;ts et taxes&nbsp;;</li>\r\n				</ul>\r\n			</li>\r\n			<li>\r\n				Assistance du contribuable &agrave; l&#39;occasion de v&eacute;rifications.</li>\r\n		</ul>\r\n	</div>\r\n</div>\r\n','fr',8),(4,'Nos missions de conseil et de gestion','<h5 class=\"title\">\r\n	Conseil en gestion g&eacute;n&eacute;rale :</h5>\r\n<ul class=\"default\">\r\n	<li>\r\n		Gestion pr&eacute;visionnelle&nbsp;;</li>\r\n	<li>\r\n		Analyse de co&ucirc;ts, de marges, de rentabilit&eacute;&nbsp;;</li>\r\n	<li>\r\n		Projets d&#39;investissement : co&ucirc;t et rentabilit&eacute;&nbsp;;</li>\r\n	<li>\r\n		Tableaux de bord, contr&ocirc;le budg&eacute;taire&nbsp;;</li>\r\n	<li>\r\n		Contribution &agrave; la protection du patrimoine de l&#39;entreprise&nbsp;;</li>\r\n	<li>\r\n		Etude d&#39;optimisation du profil&nbsp;;</li>\r\n	<li>\r\n		Ratios de gestion&nbsp;;</li>\r\n	<li>\r\n		Assistance dans la pr&eacute;vention et le traitement des difficult&eacute;s de l&#39;entreprise.</li>\r\n</ul>\r\n<h5 class=\"title\">\r\n	Conseil en gestion financi&egrave;re :</h5>\r\n<ul class=\"default\">\r\n	<li>\r\n		Analyse de gestion financi&egrave;re et de rentabilit&eacute; des capitaux investis&nbsp;;</li>\r\n	<li>\r\n		Ratios financiers&nbsp;;</li>\r\n	<li>\r\n		Techniques d&#39;analyse et de gestion du fonds de roulement&nbsp;;</li>\r\n	<li>\r\n		Syst&egrave;mes de cr&eacute;dit, cr&eacute;dit-bail, affacturage&nbsp;;</li>\r\n	<li>\r\n		Prises de participation (&eacute;tude, assistance, r&eacute;alisation pratique)&nbsp;;</li>\r\n	<li>\r\n		Constitution de dossiers de subventions, de r&eacute;duction de droits de mutation, d&#39;exon&eacute;ration de taxe professionnelle.</li>\r\n</ul>\r\n<h5 class=\"title\">\r\n	Relations avec les organismes bancaires et financiers :</h5>\r\n<ul class=\"default\">\r\n	<li>\r\n		Constitution de dossiers dans le cadre de cr&eacute;ations et d&#39;implantations d&#39;entreprise&nbsp;;</li>\r\n	<li>\r\n		Analyse de structure financi&egrave;re&nbsp;;</li>\r\n	<li>\r\n		&Eacute;tablissement de plans de tr&eacute;sorerie&nbsp;;</li>\r\n	<li>\r\n		Construction de plans de financement &agrave; moyen et long terme&nbsp;;</li>\r\n	<li>\r\n		Choix des modes de financement et &eacute;tablissement de dossiers de cr&eacute;dit&nbsp;;</li>\r\n	<li>\r\n		Tableau d&#39;emplois et de ressources (historique et pr&eacute;visionnel)&nbsp;;</li>\r\n	<li>\r\n		Centrale des bilans.</li>\r\n</ul>\r\n','fr',4),(5,'Nos missions sociales','<h5 class=\"title\">Droit du travail :</h5>\r\n					<ul class=\"default\">\r\n						<li>Application de la législation et de la réglementation du travail&nbsp;;</li>\r\n						<li>Consultations sur les problèmes spécifiques au droit du travail&nbsp;;</li>\r\n						<li>Étude et rédaction de contrats de travail&nbsp;;</li>\r\n\r\n						<li>Problèmes de licenciement&nbsp;;</li>\r\n						<li>Problèmes relatifs à la représentation dans l\'entreprise, délégués du personnel, comité d\'entreprise, droit syndical&nbsp;;</li>\r\n						<li>Étude et application des textes relatifs à la participation des travailleurs aux fruits de l\'expansion&nbsp;;</li>\r\n						<li>Système d\'intéressement (mise en œuvre, contrôle)&nbsp;;</li>\r\n						<li>Formation professionnelle continue.</li>\r\n\r\n					</ul>\r\n					\r\n					<h5 class=\"title\">Prévoyance sociale :</h5>\r\n					<ul class=\"default\">\r\n						<li>Consultation sur les problèmes spécifiques à la sécurité sociale et à la prévoyance sociale (notamment les régimes de retraite)&nbsp;;</li>\r\n						<li>Établissement des bulletins de paie et de la comptabilité des salaires&nbsp;;</li>\r\n						<li>Établissement des déclarations de cotisations sociales&nbsp;;</li>\r\n\r\n						<li>Assistance à l\'occasion de réclamation et de vérification des administrations sociales.</li>\r\n					</ul>\r\n					\r\n					<h5 class=\"title\">Bilan social :</h5>\r\n					<ul class=\"default\">\r\n						<li>Étude et mise en place&nbsp;;</li>\r\n						<li>Contrôle.</li>\r\n					</ul>\r\n','fr',3),(6,'Nos missions informatiques','<h5 class=\"title\">\r\n	Organisation d&#39;un syst&egrave;me comptable classique :</h5>\r\n<ul class=\"default\">\r\n	<li>\r\n		Mode de saisie des informations&nbsp;;</li>\r\n	<li>\r\n		Choix du support de saisie.</li>\r\n</ul>\r\n<h5 class=\"title\">\r\n	Mise en place d&#39;un syst&egrave;me informatique r&eacute;parti ou autonome destin&eacute; &agrave; offrir &agrave; l&#39;entreprise des prestations tr&egrave;s &eacute;labor&eacute;es en mati&egrave;re de gestion comptable, de gestion commerciale et de production :</h5>\r\n<ul class=\"default\">\r\n	<li>\r\n		&Eacute;tude d&#39;opportunit&eacute;&nbsp;;</li>\r\n	<li>\r\n		R&eacute;daction d&#39;un plan-type de cahier des charges de consultation&nbsp;;</li>\r\n	<li>\r\n		D&eacute;pouillement des offres&nbsp;;</li>\r\n	<li>\r\n		Participation &agrave; l&#39;&eacute;quipe informatique.</li>\r\n</ul>\r\n<h5 class=\"title\">\r\n	R&eacute;vision des comptabilit&eacute;s sur ordinateur</h5>\r\n','fr',3),(7,'Nos missions d\'organisation','<h5 class=\"title\">Conseil en organisation générale :</h5>\r\n					<ul class=\"default\">\r\n						<li>Aide à la définition de la politique générale&nbsp;;</li>\r\n						<li>Organigramme, structure, définition de fonctions&nbsp;;</li>\r\n						<li>Décentralisation, implantation, regroupements, diversification.</li>\r\n\r\n					</ul>\r\n					\r\n					<h5 class=\"title\">Conseil en organisation administrative :</h5>\r\n					<ul class=\"default\">\r\n						<li>Liaisons inter-services, circuit des documents et de l\'information&nbsp;;</li>\r\n						<li>Mécanisation des opérations, choix du matériel&nbsp;;</li>\r\n						<li>Assistance au recrutement&nbsp;;</li>\r\n\r\n						<li>Informatisation&nbsp;;</li>\r\n						<li>Analyse et organisation de circuits.</li>\r\n					</ul>\r\n					\r\n					<h5 class=\"title\">Conseil en organisation commerciale :</h5>\r\n					<ul class=\"default\">\r\n						<li>Aide à la définition de la politique et de l\'organisation commerciales&nbsp;;</li>\r\n\r\n						<li>Étude de prix&nbsp;;</li>\r\n						<li>Exportation, statistiques commerciales.</li>\r\n					</ul>\r\n','fr',4),(8,'Nos missions auprès des collectivités locales','<ul class=\"default\">\r\n	<li>\r\n		Diagnostic financier d&#39;une collectivit&eacute; territoriale et de ses satellites&nbsp;;</li>\r\n	<li>\r\n		Aide &agrave; la conception et au suivi des proc&eacute;dures de contr&ocirc;le interne&nbsp;;</li>\r\n	<li>\r\n		&Eacute;laboration d&#39;un plan pluriannuel d&#39;investissement&nbsp;;</li>\r\n	<li>\r\n		Mise en place de proc&eacute;dures de recensement, de valorisation et de suivi du patrimoine&nbsp;;</li>\r\n	<li>\r\n		Assistance lors du choix des formes de coop&eacute;rations intercommunales&nbsp;;</li>\r\n	<li>\r\n		Assistance lors de la validation du rapport annuel du d&eacute;l&eacute;gataire&nbsp;;</li>\r\n	<li>\r\n		Audit et analyse de contrat de d&eacute;l&eacute;gation de service public&nbsp;;</li>\r\n	<li>\r\n		Mise en place des proc&eacute;dures d&#39;attribution, de contr&ocirc;le et de suivi des subventions vers&eacute;es&nbsp;;</li>\r\n	<li>\r\n		&Eacute;valuation des politiques publiques locales&nbsp;;</li>\r\n	<li>\r\n		Assistance lors d&#39;interventions &eacute;conomiques de la collectivit&eacute; locale.</li>\r\n</ul>\r\n','fr',4);
