INSERT INTO `dm_widget_translation` (`id`, `value`, `lang`) VALUES (1,'{\"text\":\"Page introuvable\",\"tag\":\"h1\"}','fr'),(2,'[]','fr'),(5,'{\"separator\":\">\",\"includeCurrent\":true,\"includeInactivePages\":true}','fr'),(7,'[]','fr'),(8,'{\"menuType\":\"dropdown\",\"ulClass\":\"\",\"menuName\":\"\",\"liClass\":\"\",\"menuClass\":\"\",\"items\":[{\"link\":\"page:1\",\"text\":\"Accueil\",\"secure\":0,\"nofollow\":0,\"depth\":\"0\",\"target\":\"\"},{\"link\":\"page:1658\",\"text\":\"Le Cabinet\",\"secure\":0,\"nofollow\":0,\"depth\":\"1\",\"target\":\"\"},{\"link\":\"page:1595\",\"text\":\"Nos Services\",\"secure\":0,\"nofollow\":0,\"depth\":\"1\",\"target\":\"\"},{\"link\":\"page:1548\",\"text\":\"Contact\",\"secure\":0,\"nofollow\":0,\"depth\":\"0\",\"target\":\"\"},{\"link\":\"page:3248\",\"text\":\"Plan d\'acc\\u00e8s\",\"secure\":0,\"nofollow\":0,\"depth\":\"0\",\"target\":\"\"}]}','fr'),(11,'{\"orderField\":\"position\",\"orderType\":\"asc\",\"maxPerPage\":20,\"sectionFilter\":null,\"navTop\":false,\"navBottom\":false}','fr'),(17,'[]','fr'),(35,'{\"html\":\"<h2>\\n\\t<span style=\\\"color: rgb(255, 140, 0);\\\">Toutes nos missions<\\/span><\\/h2>\\n<div id=\\\"content-area\\\">\\n\\t<p>\\n\\t\\tL&#39;expert-comptable, <b>sp&eacute;cialiste pluridisciplinaire<\\/b>, dispose de comp&eacute;tences &eacute;tendues pour vous suivre et vous conseiller durant toute la vie de votre entreprise.<\\/p>\\n\\t<p>\\n\\t\\tNotre cabinet met &agrave; votre disposition un <b>savoir-faire<\\/b> reconnu en mati&egrave;re <b>financi&egrave;re, fiscale, sociale, juridique, informatique et de gestion<\\/b>.<br \\/>\\n\\t\\tEn relation constante avec les organes d&eacute;cisionnels de votre entreprise, il vous assure une prestation fiable et rapide ainsi qu&#39;un conseil permanent dans les domaines cruciaux de la vie de votre entreprise. Nous nous impliquons directement dans votre strat&eacute;gie de r&eacute;ussite et nous nous engageons &agrave; vos c&ocirc;t&eacute;s afin de contribuer &agrave; faire de votre structure une entreprise dynamique et adapt&eacute;e aux besoins de votre client&egrave;le.<\\/p>\\n<\\/div>\\n\"}','fr'),(39,'{\"html\":\"<h2>\\n\\t<span class=\\\"nodeLabelBox repTarget \\\" role=\\\"treeitem\\\"><span class=\\\"nodeText editable \\\"><span>Nos missions comptables<\\/span><\\/span><\\/span><\\/h2>\\n<h4>\\n\\t<span style=\\\"color: rgb(255, 140, 0);\\\">Participation &agrave; l&#39;&eacute;tablissement des comptes :<\\/span><\\/h4>\\n<ul>\\n\\t<li>\\n\\t\\tMise en place du Plan comptable adapt&eacute; &agrave; l&#39;entreprise&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tSurveillance, tenue de comptabilit&eacute;s&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tContr&ocirc;le des services comptables&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tMise en place et suivi de la comptabilit&eacute; analytique d&#39;exploitation&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tAssistance comptable de toute nature&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tEtablissement des situations p&eacute;riodiques, des bilans et comptes de r&eacute;sultat&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tD&eacute;livrance du visa aux adh&eacute;rents des centres de gestion agr&eacute;&eacute;s&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tEtablissement de situations pr&eacute;visionnelles&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tEtablissement des comptes de groupes, consolidation des bilans et des comptes&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\t&Eacute;tablissement du tableau de financement&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tAssistance du comit&eacute; d&#39;entreprise.<\\/li>\\n<\\/ul>\\n<p>\\n\\t&nbsp;<\\/p>\\n<h4>\\n\\t<span style=\\\"color: rgb(255, 140, 0);\\\">R&eacute;vision, certification :<\\/span><\\/h4>\\n<ul>\\n\\t<li>\\n\\t\\tR&eacute;vision contractuelle des comptes&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tR&eacute;vision des proc&eacute;dures de contr&ocirc;le interne&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tR&eacute;vision l&eacute;gale des comptes (commissariat aux comptes)&nbsp;;<\\/li>\\n\\t<li>\\n\\t\\tR&eacute;vision avant prise de participation&nbs