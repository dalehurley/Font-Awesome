INSERT INTO `sid_cabinet_equipe_translation` (`id`, `title`, `statut`, `tel`, `fax`, `gsm`, `email`, `text`, `lang`, `version`) VALUES (1,'Jean-Pierre Legrand','Fondateur ','01 23 45 67 89','','','legrand@lesiteducabinettenor.com','<p>\r\n	<span class=\"vcard hcard-equipe wrapper\"><span class=\"description\"><span class=\"value\">Apr&egrave;s avoir assur&eacute; des fonctions de Directeur d&#39;Agence pendant quinze ans, dans l&#39;une des principales banques fran&ccedil;aises, Jean-Pierre Legrand a &eacute;t&eacute; <b>Directeur de R&eacute;gion dans un Cabinet Conseil</b> sp&eacute;cialis&eacute; sur le march&eacute; des professions lib&eacute;rales et les PME. Ces diff&eacute;rentes exp&eacute;riences l&#39;ont conduit &agrave; cr&eacute;er, en 1999, sa propre activit&eacute; de conseil.<br />\r\n	Jean-Pierre Legrand est membre de la Chambre des Ind&eacute;pendants du Patrimoine. Il est titulaire d&#39;un DESS Gestion de Patrimoine &agrave; l&#39;universit&eacute; de Clermont-Ferrand (AUREP). </span></span></span></p>\r\n','fr',2),(2,'Monsieur Marc Filibert','Collaborateur ','05 49 60 20 61','','','filibert@lesiteducabinettenor.com','<p>\r\n	<span class=\"vcard hcard-equipe wrapper\"><span class=\"description\"><span class=\"value\">Marc Filibert a int&eacute;gr&eacute; le cabinet en 2003. Dipl&ocirc;m&eacute; de l&#39;ESC Toulouse, Il est en charge de la commercialisation des produits du cabinet (immobilier, assurance-vie...) et de l&#39;administration du site internet. </span></span></span></p>\r\n','fr',2),(3,'Madame Catherine Michel','Collaborateur','05 49 60 20 63','','','michel@lesiteducabinettenor.com','<p>\r\n	<span class=\"vcard hcard-equipe wrapper\"><span class=\"description\"><span class=\"value\">Catherine Michel a int&eacute;gr&eacute; le cabinet en 2009. Elle est titulaire d&#39;un DESS en Gestion de Patrimoine et poss&egrave;de plus de 10 ans d&#39;exp&eacute;rience bancaire. Elle a&nbsp;en charge le back-office et la gestion du cabinet. </span></span></span></p>\r\n','fr',2);
