INSERT INTO `sid_cabinet_accueil_translation_version` (`id`, `lang`, `presentation`, `mission`, `equipe`, `version`) VALUES (1,'fr','<p>\r\n	Le Nom du Cabinet est une soci&eacute;t&eacute; d&#39;expertise-comptable et de commissariat aux comptes implant&eacute;e &agrave; Paris et dans le d&eacute;partement de la Vienne.<br />\r\n	Elle est dirig&eacute;e par 2 associ&eacute;s, messieurs Emmanuel Legrand et Alain Lefort, entour&eacute;s d&#39;une &eacute;quipe de professionnels qui sont &agrave; votre &eacute;coute pour vous conseiller, construire, d&eacute;velopper.<br />\r\n	Forte de plus de 600 clients, le Nom du Cabinet intervient dans tous les secteurs d&#39;activit&eacute; : commerce, industrie, professions lib&eacute;rales, associations...</p>\r\n<p>\r\n	<a href=\"le-cabinet/presentation\">en savoir plus sur Le Nom du Cabinet<br />\r\n	</a></p>\r\n','<p>\r\n	L&#39;expert-comptable, sp&eacute;cialiste pluridisciplinaire, dispose de comp&eacute;tences &eacute;tendues pour vous suivre et vous conseiller durant toute la vie de votre entreprise.<br />\r\n	Notre cabinet met &agrave; votre disposition un savoir-faire reconnu en mati&egrave;re financi&egrave;re, fiscale, sociale, juridique, informatique et de gestion.</p>\r\n<p>\r\n	<a href=\"nos-missions\">en savoir plus sur nos missions<br />\r\n	</a></p>\r\n','',1),(1,'fr','<p>\r\n	Le Nom du Cabinet est une soci&eacute;t&eacute; d&#39;expertise-comptable et de commissariat aux comptes implant&eacute;e &agrave; Paris et dans le d&eacute;partement de la Vienne.<br />\r\n	Elle est dirig&eacute;e par 2 associ&eacute;s, messieurs Emmanuel Legrand et Alain Lefort, entour&eacute;s d&#39;une &eacute;quipe de professionnels qui sont &agrave; votre &eacute;coute pour vous conseiller, construire, d&eacute;velopper.<br />\r\n	Forte de plus de 600 clients, le Nom du Cabinet intervient dans tous les secteurs d&#39;activit&eacute; : commerce, industrie, professions lib&eacute;rales, associations...</p>\r\n','<p>\r\n	L&#39;expert-c