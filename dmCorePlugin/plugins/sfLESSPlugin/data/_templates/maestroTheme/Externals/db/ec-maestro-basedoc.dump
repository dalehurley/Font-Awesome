INSERT INTO `sid_article_translation` (`id`, `title`, `chapeau`, `lang`, `created_at`, `updated_at`) VALUES (18674,'5 avril 2012','\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de mars 2012 pays au plus tard le 31 mars 2012.\nArtisans, commerants et industriels : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'assurance vieillesse, d\'invalidit-dcs, d\'allocations familiales, de la CSG et de la CRDS.\nProfessionnels libraux : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'allocations familiales, de la CSG et de la CRDS.\n\n','fr','2012-04-05 16:40:00','2012-03-06 16:41:55'),(18675,'8 avril 2012','\nEmployeurs occupant au moins 50 salaris : relev des contrats de travail conclus ou rsilis en mars 2012  transmettre  la Direction de l\'animation de la recherche, des tudes et des statistiques (DARES).\n\n','fr','2012-04-08 16:42:00','2012-03-06 16:43:18'),(18676,'13 avril 2012','\nAssujettis  la TVA ralisant des oprations intracommunautaires : dpt auprs des douanes de la dclaration dchanges de biens et de la dclaration europenne des services pour les oprations intervenues en mars 2012.\n\n','fr','2012-04-13 16:44:00','2012-03-06 16:44:53'),(18677,'15 avril 2012','\nSocits soumises  limpt sur les socits ayant clos leur exercice le 31 dcembre 2011 ou qui nont pas cltur dexercice au titre de lanne 2011 : paiement du solde de limpt sur les socits et de la contribution sociale.\nEntreprises de 9 salaris au plus nayant pas opt pour le paiement mensuel des charges sociales : dclaration et paiement des charges sociales sur les salaires du 1er trimestre 2012.\nEntreprises dont leffectif est suprieur  9 et infrieur  50 salaris, et entreprises de 9 salaris au plus ayant opt pour le paiement mensuel des charges sociales : dclaration et paiement des charges sociales sur les salaires de mars 2012.\nEmployeurs assujettis  la taxe sur les salaires : paiement de la taxe sur les salaires pays en mars 2012 lorsque le total des sommes dues au titre de 2011 excdait 4 000 , ou au cours du 1er trimestre 2012 lorsque le total des sommes dues au titre de 2011 tait compris entre 1 000 et 4 000 .\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de mars 2012 pays dans les 10 premiers jours davril 2012.\n\n','fr','2012-04-15 16:46:00','2012-03-06 16:47:10'),(18678,'20 avril 2012','\nArtisans, commerants et industriels : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'assurance vieillesse, d\'invalidit-dcs, d\'allocations familiales, de la CSG, de la CRDS (sur demande).\nProfessionnels libraux : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'allocations familiales, de la CSG, de la CRDS (sur demande).\n\n','fr','2012-04-20 16:48:00','2012-03-06 16:49:03'),(18679,'24 avril 2012','\nRedevables et assujettis partiels  la TVA : rgularisation des dductions opres en 2011 et en dbut 2012 sur la base du coefficient de dduction.\n\n','fr','2012-04-24 16:49:00','2012-03-06 16:50:54'),(18680,'25 avril 2012','\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de mars 2012 pays entre le 11 et le 20 avril 2012.\n\n','fr','2012-04-25 16:52:00','2012-03-06 16:52:58'),(18681,'30 avril 2012','\nEntreprises appliquant la participation dont lexercice sest clos au 31 dcembre 2011 : versement des sommes attribues au titre de la participation 2011 aux salaris qui optent pour le versement immdiat.\nVersement de rgularisation de la taxe dapprentissage, le cas chant, auprs du service des impts des entreprises sur un bordereau n 2485.\n\n','fr','2012-04-30 16:53:00','2012-03-06 16:54:34'),(18682,'14 mai 2012','\nAssujettis  la TVA ayant ralis des oprations intracommunautaires : dpt auprs des douanes de la dclaration dchanges de biens et de la dclaration europenne des services pour les oprations intervenues en avril 2012.\n\n','fr','2012-05-14 17:26:00','2012-03-06 17:26:59'),(18683,'3 mai 2012','\nDclaration annuelle des honoraires, commissions et courtages verss en 2011 (DADS 2).\nDpt des principales dclarations professionnelles annuelles :\n- dclaration des rsultats des titulaires de bnfices industriels et commerciaux, de bnfices agricoles et de bnfices non commerciaux imposs daprs un rgime rel ;\n- dclaration de participation  la formation professionnelle continue n 2483 ;\n- dclaration de participation-construction n 2080 ;\n- le cas chant, dclaration n 1447-M relative  la cotisation foncire des entreprises ;\n- dclaration n 1330-CVAE relative  la cotisation sur la valeur ajoute des entreprises (CVAE) ;\n- dclaration n1329-DEF de liquidation et de rgularisation de la CVAE 2011 et paiement de limpt correspondant ;\n- dclaration rcapitulative de la taxe sur les surfaces commerciales n 3351-SD.\nTitulaires de bnfices non commerciaux relevant du rgime micro-BNC : option pour le rgime de la dclaration contrle au titre de 2011-2012.\nRedevables de la TVA soumis au rgime simplifi dimposition : dclaration annuelle de TVA (CA 12) et de rgularisation de TVA.\nSocits soumises  limpt sur les socits nayant clos aucun exercice en 2011 ou ayant clos leur exercice le 31 dcembre 2011 ou le 31 janvier 2012 : dpt de la dclaration des rsultats n 2065 et des documents annexes.\nSocits civiles immobilires non soumises  limpt sur les socits : dclaration de rsultats de 2011 sur limprim n 2072 en double exemplaire.\n\n','fr','2012-05-03 17:20:00','2012-03-06 17:21:36'),(18684,'5 mai 2012','\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires davril 2012 pays au plus tard le 30 avril 2012.\nArtisans, commerants et industriels : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'assurance vieillesse, d\'invalidit-dcs, d\'allocations familiales, de la CSG et de la CRDS.\nProfessionnels libraux n