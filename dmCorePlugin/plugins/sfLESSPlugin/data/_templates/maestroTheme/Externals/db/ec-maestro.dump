INSERT INTO `sid_cabinet_page_cabinet_translation_version` (`id`, `lang`, `title`, `resume`, `text`, `version`) VALUES (1,'fr','Prsentation du Cabinet',NULL,'<p>Le Nom du Cabinet est une soci&eacute;t&eacute; d&#39;expertise-comptable et de commissariat aux comptes implant&eacute;e dans les d&eacute;partements de la Vienne et de la Charente depuis 1993. Nos 2 bureaux sont situ&eacute;s sur Poitiers et Ruffec.<br />Le Nom du Cabinet est dirig&eacute; par 2 associ&eacute;s, Emmanuel Legrand et Alain Lefort, entour&eacute;s d&#39;une &eacute;quipe de 10 collaborateurs, juristes, comptables et experts qui sont &agrave; votre &eacute;coute pour vous conseiller, construire, d&eacute;velopper.<br />Forts de nos 600 clients et de nos 16 ans d&rsquo;exp&eacute;rience, nous intervenons et ma&icirc;trisons des environnements vari&eacute;s que ce soit en termes d&rsquo;effectifs par exemple ou de secteur d&rsquo;activit&eacute; (commerces, industries, professions lib&eacute;rales, BTP&hellip;).<br /><br />La conception que nous avons de notre m&eacute;tier est d&rsquo;&ecirc;tre proche de nos clients, sur le terrain, toujours &agrave; votre disposition. Chacun de nos clients a un interlocuteur unique qui les suit, se forme r&eacute;guli&egrave;rement pour &ecirc;tre toujours au fait des &eacute;volutions impactant votre gestion, organise le recours &agrave; toutes nos autres comp&eacute;tences internes ou externes et sait se rendre disponible.<br /><br />Contactez-nous pour en savoir plus sur notre accompagnement et nos services.</p>',1),(1,'fr','Prsentation du Cabinet','<p>\r\n	{{nomcabinet}} est une soci&eacute;t&eacute; d&#39;expertise-comptable et de commissariat aux comptes implant&eacute;e &agrave; Paris et dans le d&eacute;partement de la Vienne.<br />\r\n	Elle est dirig&eacute;e par 2 associ&eacute;s, messieurs Emmanuel Legrand et Alain Lefort, entour&eacute;s d&#39;une &eacute;quipe de professionnels qui sont &agrave; votre &eacute;coute pour vous conseiller, construir