INSERT INTO `sid_index_sites_utiles_translation` (`id`, `title`, `description`, `lang`, `created_at`, `updated_at`) VALUES (1,'Nos sites utiles','<p>\r\n	Vous trouverez ici les sites que nous consid&eacute;rons comme utiles.&nbsp;Des sites gouvernementaux en passant par des sites utiles &agrave; la vie de votre entreprise, ou des sites d&eacute;di&eacute;s aux BNC.</p>\r\n<p>\r\n	Si vous avez d&#39;autres suggestions de sites int&eacute;ressants, n&#39;h&eacute;sitez pas &agrave; contacter St&eacute;phanie Lesage, notre assistante de direction, pour lui en faire part : <a href=\"mailto:contact@expertinfos.com?subject=Test%20effectu%C3%A9%20sur%20le%20site%20de%20d%C3%A9monstration%20T%C3%A9nor%20expert\">slesage@le-cabinet-tenor-expert.com</a></p>\r\n<p>\r\n	<em>T&eacute;nor est un mod&egrave;le de site internet con&ccedil;u par EXPERT INFOS sp&eacute;cifiquement pour les cabinets d&#39;expertise comptable. Il inclut des rubriques sur-mesure comme cette rubrique pr&eacute;sentant des sites utiles que vous pilotez directement de votre ordinateur. Contactez nos &eacute;quipes pour en savoir plus&nbsp; : <a href=\"mailto:contact@expertinfos.com?subject=Demande%20d%27information%20suite%20visite%20du%20site%20internet%20TENOR\">contact@expertinfos.com</a> / 05 49 60 26 47</em></p>\r\n','fr','2011-09-14 06:07:12','2011-09-28 13:52:25');
