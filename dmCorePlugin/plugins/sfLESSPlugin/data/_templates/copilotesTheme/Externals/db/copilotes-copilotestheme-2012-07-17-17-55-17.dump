INSERT INTO `sid_article_translation` (`id`, `title`, `chapeau`, `lang`, `created_at`, `updated_at`) VALUES (18840,'5 juin 2012','\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de mai 2012 pays au plus tard le 31 mai 2012.\nArtisans, commerants et industriels : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'assurance vieillesse, d\'invalidit-dcs, d\'allocations familiales, de la CSG et de la CRDS.\nProfessionnels libraux : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'allocations familiales, de la CSG et de la CRDS.\n\n','fr','2012-06-05 12:00:00','2012-04-10 12:01:26'),(18841,'8 juin 2012','\nEmployeurs occupant au moins 50 salaris : relev des contrats de travail conclus ou rsilis en mai 2012  transmettre  la Direction de lanimation de la recherche, des tudes et des statistiques (DARES).\n\n','fr','2012-06-08 12:03:00','2012-04-10 12:04:20'),(18842,'12 juin 2012','\nAssujettis  la TVA ralisant des oprations intracommunautaires : dpt auprs des douanes de la dclaration dchanges de biens et de la dclaration europenne des services pour les oprations intervenues en mai 2012.\n\n','fr','2012-06-12 12:05:00','2012-04-10 12:05:56'),(18843,'15 juin 2012','\nEmployeurs assujettis  la taxe sur les salaires : paiement de la taxe sur les salaires verss en mai 2012 lorsque le total des sommes dues au titre de 2011 excdait 4 000 .\nSocits soumises  limpt sur les socits ayant clos leur exercice le 29 fvrier 2012 : paiement du solde de limpt sur les socits et de la contribution sociale.\nSocits soumises  limpt sur les socits : versement de lacompte dIS.\nRedevables de limpt de solidarit sur la fortune (ISF) dont le patrimoine net taxable est suprieur ou gal  3 millions deuros : dpt de la dclaration annuelle n 2725 et versement de limpt correspondant.\nAssujettis  la cotisation foncire des entreprises (CFE) : versement, le cas chant, de lacompte de CFE venu  chance le 31mai 2012 (sauf en cas de mensualisation).\nAssujettis  la cotisation sur la valeur ajoute des entreprises (CVAE) : paiement, le cas chant, de lacompte de CVAE.\nDclaration n 3350-SD et paiement de la taxe sur les surfaces commerciales.\nEntreprises dont leffectif est suprieur  9 et infrieur  50 salaris, et entreprises de 9 salaris au plus ayant opt pour le paiement mensuel des charges sociales : dclaration et paiement des charges sociales sur les salaires de mai 2012.\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de mai 2012 pays dans les 10 premiers jours de juin 2012.\n\n','fr','2012-06-15 12:09:00','2012-04-10 12:12:45'),(18844,'20 juin 2012','\nArtisans, commerants et industriels : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'assurance vieillesse, d\'invalidit-dcs, d\'allocations familiales, de la CSG et de la CRDS (sur demande).\nProfessionnels libraux : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, dallocations familiales, de la CSG et dela CRDS (sur demande).\n\n','fr','2012-06-20 12:14:00','2012-04-10 12:16:59'),(18845,'25 juin 2012','\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de mai 2012 pays entre le 11 et le 20 juin 2012.\n\n','fr','2012-06-25 12:18:00','2012-04-10 12:19:57'),(18846,'30 juin 2012','\nContribuables ayant opt pour le paiement mensuel de leur impt sur le revenu et/ou de leurs impts locaux : dernire facult de rsiliation de cette option pour lanne en cours.\nContribuables nayant pas opt pour le paiement mensuel de leur impt sur le revenu et de leurs impts locaux : dernire facult doption pour lanne en cours.\nSocits soumises  limpt sur les socits ayant clos leur exercice le 31 mars 2012 : dpt de la dclaration annuelle des rsultats et des documents annexes.\n\n','fr','2012-06-30 12:21:00','2012-04-10 12:22:10'),(18847,'Juin 2012','\nDclaration et paiement de la TVA correspondant aux oprations de mai 2012 et, ventuellement, demande de remboursement du crdit de TVA au titre du mois de mai 2012.\n\n','fr','0000-00-00 00:00:00','2012-04-10 11:58:41'),(18848,'Juillet 2012','\nDclaration et paiement de la TVA correspondant aux oprations de juin 2012 ou du 2e trimestre 2012 et, ventuellement, demande de remboursement du crdit de TVA au titre du 2e trimestre 2012 ou du mois de juin 2012.\n\n','fr','0000-00-00 00:00:00','2012-05-29 11:42:35'),(18849,'5 juillet 2012','\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de juin 2012 pays au plus tard le 30 juin 2012.\nArtisans, commerants et industriels : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'assurance vieillesse, d\'invalidit-dcs, d\'allocations familiales, de la CSG et de la CRDS.\nProfessionnels libraux : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'allocations familiales, de la CSG et de la CRDS.\n\n','fr','2012-07-05 11:43:00','2012-05-29 11:44:43'),(18850,'8 juillet 2012','\nEmployeurs occupant au moins 50 salaris : relev des contrats de travail conclus ou rsilis en juin 2012  transmettre  la Direction de lanimation de la recherche, des tudes et des statistiques (DARES).\n\n','fr','2012-07-08 11:45:00','2012-05-29 11:46:46'),(18851,'12 juillet 2012','\nAssujettis  la TVA ralisant des oprations intracommunautaires : dpt auprs des douanes de la dclaration dchanges de biens et de la dclaration europenne des services pour les oprations intervenues en juin 2012.\n\n','fr','2012-07-12 11:47:00','2012-05-29 11:48:43'),(18852,'15 juillet 2012','\nSocits soumises  limpt sur les socits ayant clos leur exercice le 31 mars 2012 : paiement du solde de limpt sur les socits et de la contribution sociale.\nEntreprises dont leffectif est suprieur  9 et infrieur  50 salaris, et entreprises de 9 salaris au plus ayant opt pour le paiement mensuel des charges sociales : dclaration et paiement des charges sociales sur les salaires de juin 2012.\nEmployeurs assujettis  la taxe sur les salaires : versement de la taxe sur les salaires pays en juin 2012 lorsque le total des sommes dues au titre de 2011 excdait 4 000 , ou au cours du 2e trimestre 2012 lorsque le total des sommes dues au titre de 2011 tait compris entre 1 000 et 4000 .\nEntreprises de 9 salaris au plus nayant pas opt pour le paiement mensuel des charges sociales : dclaration et paiement des charges sociales sur les salaires du 2e trimestre 2012.\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de juin 2012 pays dans les 10 premiers jours de juillet 2012.\n\n','fr','2012-07-15 11:50:00','2012-05-29 11:51:39'),(18853,'20 juillet 2012','\nArtisans, commerants et industriels : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'assurance vieillesse, d\'invalidit-dcs, d\'allocations familiales, de la CSG et de la CRDS (sur demande).\nProfessionnels libraux : paiement par prlvement mensuel des cotisations d\'assurance maladie-maternit, d\'allocations familiales, de la CSG et de la CRDS (sur demande).\n\n','fr','2012-07-20 11:52:00','2012-05-29 11:53:43'),(18854,'25 juillet 2012','\nEmployeurs occupant au moins 50 salaris : dclaration et paiement des charges sociales sur les salaires de juin 2012 pays entre le 11 et le 20 juillet 2012.\n\n','fr','2012-07-25 11:54:00','2012-05-29 11:55:34'),(18855,'31 juillet 2012','\nSocits soumises  limpt sur les socits ayant clos leur exercice le 30 avril 2012 : dpt de la dclaration annuelle des rsultats et des documents annexes.\n\n','fr','2012-07-31 11:56:00','2012-05-29 11:57:48'),(18856,'Calendrier des vacances scolaires','','fr','2012-03-07 10:08:00','2012-05-03 12:16:20'),(18909,'TPE : amnagement du rgime d\'exonration des plus-values professionnelles','Le rgime d\'exonration des plus-values professionnelles applicable aux TPE dont le mont