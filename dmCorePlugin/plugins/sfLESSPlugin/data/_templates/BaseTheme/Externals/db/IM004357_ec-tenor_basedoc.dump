INSERT INTO `sid_groupe_sites_utiles_translation` (`id`, `title`, `description`, `lang`, `created_at`, `updated_at`) VALUES (1,'Les sites d\'aide aux entreprises','Quelques sites simplifiant la vie de l&#39;entreprise au quotidien.<br />\r\n<em>T&eacute;nor est un mod&egrave;le de site internet con&ccedil;u par EXPERT INFOS sp&eacute;cifiquement pour les cabinets d&#39;expertise comptable. Il inclut des rubriques sur-mesure comme cette rubrique pr&eacute;sentant des sites utiles que vous pilotez directement de votre ordinateur. Contactez nos &eacute;quipes pour en savoir plus&nbsp; : <a href=\"mailto:contact@expertinfos.com?subject=Demande%20d%27information%20suite%20visite%20du%20site%20internet%20TENOR\">contact@expertinfos.com</a> / 05 49 60 26 47</em>','fr','2011-09-14 06:07:36','2012-03-27 16:52:57'),(5,'Les sites gouvernementaux','Ici nous mettrons &agrave; disposition les liens vers les diff&eacute;rents sites du gouvernement.<em><br />\r\nT&eacute;nor est un mod&egrave;le de site internet con&ccedil;u par EXPERT INFOS sp&eacute;cifiquement pour les cabinets d&#39;expertise comptable. Il inclut des rubriques sur-mesure comme cette rubrique pr&eacute;sentant des sites utiles que vous pilotez directement de votre ordinateur. Contactez nos &eacute;quipes pour en savoir plus&nbsp; : <a href=\"mailto:contact@expertinfos.com?subject=Demande%20d%27information%20suite%20visite%20du%20site%20internet%20TENOR\">contact@expertinfos.com</a> / 05 49 60 26 47</em> ','fr','2011-09-14 06:07:36','2012-03-27 16:53:42'),(6,'Les sites dédiés aux BNC','Professionnels lib&eacute;raux, vous trouverez dans cette rubrique les liens pour vos t&eacute;l&eacute;clarations et les sites concernant votre protection sociale.\r\n<br />\r\nSi vous avez d&#39;autres suggestions de sites int&eacute;ressants, n&#39;h&eacute;sitez pas &agrave; contacter St&eacute;phanie Lesage, notre assistante de direction, pour lui en faire part : <a href=\"mailto:contact@expertinfos.com?subject=Test%20effectu%