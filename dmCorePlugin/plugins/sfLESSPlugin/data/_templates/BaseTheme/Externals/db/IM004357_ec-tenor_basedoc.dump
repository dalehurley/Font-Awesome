INSERT INTO `sid_article_translation` (`id`, `title`, `chapeau`, `lang`, `created_at`, `updated_at`) VALUES (16698,'8 mars 2012','\nEmployeurs occupant au moins 50 salariés : relevé des contrats de travail conclus ou résiliés en février 2012 à transmettre à la Direction de l’animation de la recherche, des études et des statistiques (DARES).\n\n','fr','2012-03-08 16:00:00','2012-03-06 16:25:59'),(16699,'12 mars 2012','\nAssujettis à la TVA réalisant des opérations intracommunautaires : dépôt auprès des douanes de la déclaration d’échanges de biens et de la déclaration européenne des services pour les opérations intervenues en février 2012.\n\n','fr','2012-03-12 16:26:00','2012-03-06 16:27:45'),(16700,'15 mars 2012','\nEntreprises dont l’effectif est supérieur à 9 et inférieur à 50 salariés, et entreprises de 9 salariés au plus ayant opté pour le paiement mensuel des charges sociales : déclaration et paiement des charges sociales sur les salaires de février 2012.\nSociétés soumises à l’impôt sur les sociétés ayant clos leur exercice le 30 novembre 2011 : paiement du solde de l’impôt sur les sociétés et de la contribution sociale.\nEmployeurs assujettis à la taxe sur les salaires : paiement de la taxe sur les salaires payés en février 2012 lorsque le total des sommes dues au titre de 2011 excédait 4 000 €.\nSociétés soumises à l’impôt sur les sociétés : versement de l’acompte d’IS.\nSociétés soumises à l’impôt sur les sociétés : versement de l’IFA (imposition forfaitaire annuelle).\nEmployeurs occupant au moins 50 salariés : déclaration et paiement des charges sociales sur les salaires de février 2012 payés dans les 10 premiers jours de mars 2012.\n\n','fr','2012-03-15 16:29:00','2012-03-06 16:30:31'),(16701,'20 mars 2012','\nArtisans, commerçants et industriels : paiement par prélèvement mensuel des cotisations d\'assurance maladie-maternité, d\'assurance vieillesse, d\'invalidité-décès, d\'allocations familiales, de la CSG et de la CRDS (sur demande).\nProfessionnels libéraux : paiement par prélèvement mensuel des cotisations d\'assurance maladie-maternité, d\'allocations familiales, de la CSG et de la CRDS (sur demande).\n\n','fr','2012-03-20 16:31:00','2012-03-06 16:32:29'),(16702,'25 mars 2012','\nEmployeurs occupant au moins 50 salariés : déclaration et paiement des charges sociales sur les salaires de février 2012 payés entre le 11 et le 20 mars 2012.\n\n','fr','2012-03-25 16:33:00','2012-03-06 16:34:26'),(16703,'5 avril 2012','\nEmployeurs occupant au moins 50 salariés : déclaration et paiement des charges sociales sur les salaires de mars 2012 payés au plus tard le 31 mars 2012.\nArtisans, commerçants et industriels : paiement par prélèvement mensuel des cotisations d\'assurance maladie-maternité, d\'assurance vieillesse, d\'invalidité-décès, d\'allocations familiales, de la CSG et de la CRDS.\nProfessionnels libéraux : paiement par prélèvement mensuel des cotisations d\'assurance maladie-maternité, d\'allocations familiales, de la CSG et de la CRDS.\n\n','fr','2012-04-05 16:40:00','2012-03-06 16:41:55'),(16704,'8 avril 2012','\nEmployeurs occupant au moins 50 salariés : relevé des contrats de travail conclus ou résiliés en mars 2012 à transmettre à la Direction de l\'animation de la recherche, des études et des statistiques (DARES).\n\n','fr','2012-04-08 16:42:00','2012-03-06 16:43:18'),(16705,'13 avril 2012','\nAssujettis à la TVA réalisant des opérations intracommunautaires : dépôt auprès des douanes de la déclaration d’échanges de biens et de la déclaration européenne des services pour les opérations intervenues en mars 2012.\n\n','fr','2012-04-13 16:44:00','2012-03-06 16:44:53'),(16706,'15 avril 2012','\nSociétés soumises à l’impôt sur les sociétés ayant clos leur exercice le 31 décembre 2011 ou qui n’ont pas clôturé d’exercice au titre de l’année 2011 : paiement du solde de l’impôt sur les sociétés et de la contribution sociale.\nEntreprises de 9 salariés au plus n’ayant pas opté pour le paiement mensuel des charges sociales : déclaration et paiement des charges sociales sur les salaires du 1er trimestre 2012.\nEntreprises dont l’effectif est supérieur à 9 et inférieur à 50 salariés, et entreprises de 9 salariés au plus ayant opté pour le paiement mensuel des charges sociales : déclaration et paiement des charges sociales sur les salaires de mars 2012.\nEmployeurs assujettis à la taxe sur les salaires : paiement de la taxe sur les salaires payés en mars 2012 lorsque le total des sommes dues au titre de 2011 excédait 4 000 €, ou au cours du 1er trimestre 2012 lorsque le total des sommes dues au titre de 2011 était compris entre 1 000 et 4 000 €.\nEmployeurs occupant au moins 50 salariés : déclaration et paiement des charges sociales sur les salaires de mars 2012 payés dans les 10 premiers jours d’avril 2012.\n\n','fr','2012-04-15 16:46:00','2012-03-06 16:47:10'),(16707,'20 avril 2012','\nArtisans, commerçants et industriels : paiement par prélèvement mensuel des cotisations d\'assurance maladie-maternité, d\'assurance vieillesse, d\'invalidité-décès, d\'allocations familiales, de la CSG, de la CRDS (sur demande).\nProfessionnels libéraux : paiement par prélèvement mensuel des cotisations d\'assurance maladie-maternité, d\'allocations familiales, de la CSG, de la CRDS (sur demande).\n\n','fr','2012-04-20 16:48:00','2012-03-06 16:49:03'),(16708,'24 avril 2012','\nRedevables et assujettis partiels à la TVA : régularisation des déductions opérées en 2011 et en début 2012 sur la base du coefficient de déduction.\n\n','fr','2012-04-24 16:49:00','2012-03-06 16:50:54'),(16709,'25 avril 2012','\nEmployeurs occupant au moins 50 salariés : déclaration et paiement des charges sociales sur les salaires de mars 2012 payés entre le 11 et le 20 avril 2012.\n\n','fr','2012-04-25 16:52:00','2012-03-06 16:52:58'),(16710,'30 avril 2012','\nEntreprises appliquant la participation dont l’exercice s’est clos au 31 décembre 2011 : versement des sommes attribuées au titre de la participation 2011 aux 